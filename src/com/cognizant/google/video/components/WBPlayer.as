package com.cognizant.google.video.components {	import com.cognizant.google.video.components.controls.Slider;	import com.cognizant.google.video.components.controls.BtnToggle;	import flash.display.MovieClip;	import com.cognizant.google.video.components.controls.PlaybackController;	import flash.display.DisplayObject;	import com.cognizant.google.video.data.PlayerSettings;		/**	 * ...	 * 	 * @author ...	 * 	 * @created mm/dd/yyyy 00:00 AM	 */	public class WBPlayer extends MovieClip {				// NOTE: This should be the order of access modifiers per section.		// - PUBLIC		// - INTERNAL		// - PROTECTED		// - PRIVATE				// Please use these section dividers as the guide limit length of chars per line.		// ------------------------------- STATIC VARIABLES --------------------------------				// ----------------------------------- VARIABLES -----------------------------------		// Stage's instances.		public var container:MovieClip;		public var togglePlay:MovieClip;		public var toggleStop:MovieClip;		public var toggleMute:MovieClip;		public var mcSeekbar:MovieClip;		public var btnReplay:MovieClip;		public var toggleSkipTo0:MovieClip;		public var toggleSkipTo1:MovieClip;						private var _video:ExPlayer;		private var _ctrl:PlaybackController;				// --------------=--------------- STATIC GETTERS/SETTERS ---------------------------				// --------------------------- OVERRIDEN GETTERS/SETTERS ---------------------------				// ------------------------------- GETTERS/SETTERS ---------------------------------				// ------------------------------- STATIC METHODS ----------------------------------				// -------------------------------- CONSTRUCTOR ------------------------------------				public function WBPlayer () {			// constructor code		}						// ----------------------------- OVERRIDEN METHODS ---------------------------------				// ------------------------   OVERRIDEN EVENT HANDLERS -----------------------------				// ---------------------------------  METHODS --------------------------------------		public function init (settings:PlayerSettings):void {			if (_ctrl) _ctrl.destroy();			if (_video) _video.destroy();									_video = new ExPlayer();			_video.init(settings);			container.addChild(_video);						_ctrl = new PlaybackController(_video);						_ctrl.setPlayToggle(togglePlay);			_ctrl.setStopToggle(toggleStop);			_ctrl.setMuteToggle(toggleMute);			//_ctrl.setSkipToToggle(toggleSkipTo0, 0);			//_ctrl.setSkipToToggle(toggleSkipTo1, 1);			_ctrl.setReplayButton(btnReplay);			//_ctrl.setSeekBar(mcSeekbar.bar, mcSeekbar.hit, mcSeekbar.knob);					}				// ------------------------------ EVENT HANDLERS -----------------------------------	}	}