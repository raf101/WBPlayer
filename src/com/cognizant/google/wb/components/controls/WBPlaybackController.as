package com.cognizant.google.wb.components.controls {	import com.cognizant.google.video.components.controls.PlaybackController;	import com.cognizant.google.video.components.ExPlayer;	import com.cognizant.google.wb.WBCreative;	import flash.display.DisplayObject;	import flash.events.Event;	import flash.events.MouseEvent;		/**	 * ...	 * @author Rafael Nepomuceno	 * @created 6/17/2015 1:34 AM	 */	public class WBPlaybackController extends PlaybackController {		private var _btnFullscreen:DisplayObject;		private var _btnExitFullscreen:DisplayObject;		private var _btnSkip:DisplayObject;		private var _wb:WBCreative;				public function get btnFullscreen ():DisplayObject { return _btnFullscreen; }		public function get btnExitFullscreen ():DisplayObject { return _btnExitFullscreen; }		public function get btnSkip ():DisplayObject { return _btnSkip; }				public function WBPlaybackController (video:ExPlayer) {			super(video);			_wb = WBCreative.getInstance();		}				public function setBtnFullscreen (display:DisplayObject):void {			if (_btnFullscreen) {				_btnFullscreen.removeEventListener(MouseEvent.CLICK, fullscreenHandler);				_btnFullscreen = null;			}			if (!display) return;						display.addEventListener(MouseEvent.CLICK, fullscreenHandler);			_btnFullscreen = display;		}				public function setBtnExitFullscreen (display:DisplayObject):void {			if (_btnExitFullscreen) {				_btnExitFullscreen.removeEventListener(MouseEvent.CLICK, exitFullscreenHandler);				_btnExitFullscreen = null;			}			if (!display) return;						display.addEventListener(MouseEvent.CLICK, exitFullscreenHandler);			_btnExitFullscreen = display;		}				public function setBtnSkip (display:DisplayObject):void {			if (_btnSkip) {				_btnSkip.removeEventListener(MouseEvent.CLICK, videoSkipHandler);				_btnSkip = null;			}			if (!display) return;						display.addEventListener(MouseEvent.CLICK, videoSkipHandler);			_btnSkip = display;		}				private function videoSkipHandler ( e:MouseEvent ):void {			_video.stop();			_wb.videoSkip();		}				public function autoAdvance ():void {			_playState = true;			_muteState = false;						_video.unmute();			_video.next();		}			}}