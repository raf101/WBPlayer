package com.cognizant.google.wb.data {	import com.cognizant.google.video.data.PlayerSettings;	import com.cognizant.google.video.data.VideoData;	import com.cognizant.google.wb.config.WBConfig;	import flash.geom.Rectangle;		/**	 * ...	 * 	 * @author ...	 * 	 * @created mm/dd/yyyy 00:00 AM	 */	public class WBPlayerSettings {				// NOTE: This should be the order of access modifiers per section.		// - PUBLIC		// - INTERNAL		// - PROTECTED		// - PRIVATE				// Please use these section dividers as the guide limit length of chars per line.		// ------------------------------- STATIC VARIABLES --------------------------------				// ----------------------------------- VARIABLES -----------------------------------		private var _intro:PlayerSettings;		private var _videoSD:PlayerSettings;		private var _videoHD:PlayerSettings;				// --------------=--------------- STATIC GETTERS/SETTERS ---------------------------				public function get intro ():PlayerSettings { return _intro; }		public function get videoSD ():PlayerSettings { return _videoSD; }		public function get videoHD ():PlayerSettings { return _videoHD; }				// --------------------------- OVERRIDEN GETTERS/SETTERS ---------------------------				// ------------------------------- GETTERS/SETTERS ---------------------------------				// ------------------------------- STATIC METHODS ----------------------------------				// -------------------------------- CONSTRUCTOR ------------------------------------				public function WBPlayerSettings () {			// constructor code		}						// ----------------------------- OVERRIDEN METHODS ---------------------------------				// ------------------------   OVERRIDEN EVENT HANDLERS -----------------------------				// ---------------------------------  METHODS --------------------------------------		public function init (wbVideos:WBVideos):void {			var id:String = WBConfig.ID_VIDEO_INTRO;			var streamingData:Array = [wbVideos.intro.streaming];			var progressiveData:Array = [wbVideos.intro.progressive];			var dimension:Rectangle = new Rectangle(0, 0, 536, 300);			var startMuted:Boolean = true;			var autoPlay:Boolean = true;			var previewTime:Number = 0;			var componentInstance:Object = null; // ExPlayerAdvanced instance			var autoAdvanceVideoOnComplete:Boolean = false;			var autoDestroy:Boolean = true;						_intro = new PlayerSettings(id, streamingData, progressiveData, dimension, startMuted, autoPlay, previewTime, componentInstance, autoAdvanceVideoOnComplete, autoDestroy);												id = WBConfig.ID_VIDEO_SD;			streamingData = [				wbVideos.video1SD.streaming,				wbVideos.video2SD.streaming			];			progressiveData = [				wbVideos.video1SD.streaming,				wbVideos.video2SD.streaming			];			dimension = new Rectangle(0, 0, 536, 300);			_videoSD = new PlayerSettings(id, streamingData, progressiveData, dimension, startMuted, autoPlay, previewTime, componentInstance, autoAdvanceVideoOnComplete, autoDestroy);															id = WBConfig.ID_VIDEO_HD;			streamingData = [				wbVideos.video1HD.streaming,				wbVideos.video2HD.streaming			];			progressiveData = [				wbVideos.video1HD.streaming,				wbVideos.video2HD.streaming			];			dimension = new Rectangle(0, 0, 1280, 720);			startMuted = false;			_videoHD = new PlayerSettings(id, streamingData, progressiveData, dimension, startMuted, autoPlay, previewTime, componentInstance, autoAdvanceVideoOnComplete, autoDestroy);		}								// ------------------------------ EVENT HANDLERS -----------------------------------	}	}