package com.cognizant.google.wb.events {	import flash.events.Event;		/**	 * ...	 * 	 * @author Rafael Nepomuceno	 * 	 * @created mm/dd/yyyy 00:00 AM	 */	public class WBEvent extends Event {		public static var INTERACTED:String = 'interacted';		public static var AUTO_EXPANDED:String = 'autoExpanded';		public static var EXPANDED:String = 'expanded';		public static var COLLAPSED:String = 'collapsed';		public static var AUTO_EXPAND_END:String = 'autoExpandEnd';		public static var VIDEO_AUTOPLAY_END:String = 'videoAutoplayEnd';		public static var VIDEO_AUTO_ADVANCED:String = 'videoAutoAdvanced';		public static var ENTER_FULLSCREEN:String = 'enterFullscreen';		public static var EXIT_FULLSCREEN:String = 'exitFullscreen';						public var data:Object;				public function WBEvent ( type:String, data:Object = null, bubbles:Boolean=false, cancelable:Boolean=false ) { 			super(type, bubbles, cancelable);			this.data = data;		} 				public override function clone ():Event { 			return new WBEvent( type, data, bubbles, cancelable );		} 				public override function toString ():String { 			return formatToString( 'WBEvent', 'type', 'data', 'bubbles', 'cancelable', 'eventPhase' ); 		}	}	}