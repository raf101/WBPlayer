package com.cognizant.google.wb.events {	import flash.events.Event;		/**	 * ...	 * 	 * @author Rafael Nepomuceno	 * 	 * @created mm/dd/yyyy 00:00 AM	 */	public class WBEvent extends Event {		public static var AUTO_ADVANCE_VIDEO:String = 'autoAdvanceVideo';						public var data:Object;				public function WBEvent ( type:String, data:Object = null, bubbles:Boolean=false, cancelable:Boolean=false ) { 			super(type, bubbles, cancelable);			this.data = data;		} 				public override function clone ():Event { 			return new WBEvent( type, data, bubbles, cancelable );		} 				public override function toString ():String { 			return formatToString( 'WBEvent', 'type', 'data', 'bubbles', 'cancelable', 'eventPhase' ); 		}	}	}